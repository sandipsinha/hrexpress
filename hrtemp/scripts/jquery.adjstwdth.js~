       $("label[for='location']").width(wphonewidth);
       $("label[for='btitle']").width(wphonewidth);
       $("label[for='hr']").width(wphonewidth);
       $("label[for='comments']").width(wphonewidth);
       //$("label[for='vp']").width(btitlewidth);
       $("label[for='dept']").width(wphonewidth);
       $("label[for='manager']").width(wphonewidth);
       $("label[for='legal']").width(wphonewidth);
       $("label[for='finance']").width(wphonewidth);
       $("label[for='it_end_dt']").width(wphonewidth);
       $("label[for='empid']").width(wphonewidth);
       $("#wemail").width(max);

